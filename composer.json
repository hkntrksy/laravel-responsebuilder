{
    "name": "turksoy/responsebuilder",
    "description": "",
    "keywords": [
        "turksoy",
        "responsebuilder"
    ],
    "homepage": "https://github.com/hkntrksy/laravel-responsebuilder",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Hakan TÃ¼rksoy",
            "email": "hakanturksoy@yandex.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0",
        "illuminate/http": "^5.0|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Turksoy\\ResponseBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Turksoy\\ResponseBuilder\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Turksoy\\ResponseBuilder\\ResponseBuilderServiceProvider"
            ],
            "aliases": {
                "ResponseBuilder": "Turksoy\\ResponseBuilder\\ResponseBuilderFacade"
            }
        }
    }
}
